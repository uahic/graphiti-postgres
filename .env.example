# PostgreSQL Connection Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password
POSTGRES_DATABASE=postgres

# Graphiti Configuration
GRAPHITI_GROUP_ID=default
GRAPHITI_MIN_POOL_SIZE=2
GRAPHITI_MAX_POOL_SIZE=10

# OpenAI Configuration (for Graphiti)
OPENAI_API_KEY=your_openai_api_key

# Optional: Azure OpenAI
# AZURE_OPENAI_API_KEY=your_azure_key
# AZURE_OPENAI_ENDPOINT=your_azure_endpoint

# Optional: Vector Embedding Dimension
# EMBEDDING_DIMENSION=1536  # OpenAI default

# Index Names (optional customization)
# ENTITY_INDEX_NAME=entities
# EPISODE_INDEX_NAME=episodes
# COMMUNITY_INDEX_NAME=communities
# ENTITY_EDGE_INDEX_NAME=entity_edges
